		{
			"name": "Pre-Check Compatible Groups",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response has correct structure\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData).to.have.property('status');",
							"    pm.expect(jsonData).to.have.property('suggestions');",
							"    pm.expect(jsonData).to.have.property('totalSuggestions');",
							"    pm.expect(jsonData).to.have.property('hasCompatibleGroups');",
							"    pm.expect(jsonData).to.have.property('message');",
							"});",
							"",
							"pm.test(\"Status is success\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.status).to.eql('success');",
							"});",
							"",
							"pm.test(\"Suggestions is an array\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.suggestions).to.be.an('array');",
							"});",
							"",
							"pm.test(\"Total suggestions matches array length\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.totalSuggestions).to.eql(jsonData.suggestions.length);",
							"});",
							"",
							"if (pm.response.json().suggestions.length > 0) {",
							"    pm.test(\"Each suggestion has required properties\", function () {",
							"        var jsonData = pm.response.json();",
							"        jsonData.suggestions.forEach(function(suggestion) {",
							"            pm.expect(suggestion).to.have.property('groupId');",
							"            pm.expect(suggestion).to.have.property('tripName');",
							"            pm.expect(suggestion).to.have.property('groupName');",
							"            pm.expect(suggestion).to.have.property('compatibilityScore');",
							"            pm.expect(suggestion).to.have.property('currentMembers');",
							"            pm.expect(suggestion).to.have.property('maxMembers');",
							"            pm.expect(suggestion).to.have.property('createdBy');",
							"        });",
							"    });",
							"",
							"    pm.test(\"Compatibility scores are within valid range\", function () {",
							"        var jsonData = pm.response.json();",
							"        jsonData.suggestions.forEach(function(suggestion) {",
							"            pm.expect(suggestion.compatibilityScore).to.be.within(0, 1);",
							"        });",
							"    });",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
